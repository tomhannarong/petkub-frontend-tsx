           style={{ cursor: loading ? 'not-allowed' : 'pointer' }}\n          >\n            {loading ? (\n              <Loader\n                type='Oval'\n                color='white'\n                height={30}\n                width={30}\n                timeout={30000}\n              />\n            ) : (\n              'Submit'\n            )}\n          </Button>\n\n          {error && (\n            <StyledError>\n              {error.graphQLErrors[0]?.message || 'Sorry, something went wrong'}\n            </StyledError>\n          )}\n        </StyledForm>\n\n        {data && (\n          <StyledInform>\n            <p>{data.requestResetPassword?.message}</p>\n          </StyledInform>\n        )}\n      </FormContainer>\n    </Modal>\n  )\n}\n\nexport default RequestResetPassword"]},"metadata":{},"sourceType":"module"}